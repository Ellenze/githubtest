{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azureworkshopdatafactory"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"az_adls_azureworkshopdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'az_adls_azureworkshopdatalake'"
		},
		"az_adls_workshop_datalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_adls_workshop_datalake'"
		},
		"az_sql_adventruework_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_sql_adventruework'"
		},
		"az_sql_azureworkshopsql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_sql_azureworkshopsql'"
		},
		"az_adls_azureworkshopdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://azworkshopdatalake.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azureworkshopdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"az_adls_azureworkshopdatalake_accountKey": {
						"value": "[parameters('az_adls_azureworkshopdatalake_accountKey')]"
					},
					"az_adls_workshop_datalake_connectionString": {
						"value": "[parameters('az_adls_workshop_datalake_connectionString')]"
					},
					"az_sql_adventruework_connectionString": {
						"value": "[parameters('az_sql_adventruework_connectionString')]"
					},
					"az_sql_azureworkshopsql_connectionString": {
						"value": "[parameters('az_sql_azureworkshopsql_connectionString')]"
					},
					"az_adls_azureworkshopdatalake_properties_typeProperties_url": {
						"value": "[parameters('az_adls_azureworkshopdatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}